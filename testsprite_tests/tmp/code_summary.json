      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {
          "title": "UI Management API",
          "version": "1.0.0"
        },
        "paths": {
          "/ui/app-list": {
            "get": {
              "summary": "Get list of installed apps for UI display",
              "responses": {
                "200": {"description": "App list data"}
              }
            }
          }
        }
      }
    },
    {
      "name": "Crash Prevention System",
      "description": "Advanced crash detection and prevention mechanisms for stable app cloning operations",
      "files": [
        "android/app/src/main/kotlin/com/multispace/app/multispace_cloner1/CrashHandler.kt",
        "android/app/src/main/kotlin/com/multispace/app/multispace_cloner1/MainActivity.kt"
      ],
